{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/cross-compiler/lib/gcc/i686-elf/13.0.1/include",
                "${workspaceFolder}/src/kernel"
            ],
            "defines": [],
            "compilerPath": "${workspaceFolder}/cross-compiler/bin/i686-elf-g++",
            "cStandard": "c99",
            "cppStandard": "gnu++23",
            "compilerArgs": [
                "-std=gnu++23",
                "-ffreestanding ",
                "-O2 ",
                "-Wall ",
                "-Wextra"
            ],
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}